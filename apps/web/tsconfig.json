{
  "extends": "@dms/tsconfig/base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "allowJs": false,
    "noEmit": true,
    "allowArbitraryExtensions": true,
    "incremental": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.d.ts", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
