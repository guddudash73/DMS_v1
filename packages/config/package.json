{
  "name": "@dms/config",
  "version": "0.0.3",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/env.js",
  "module": "./dist/env.js",
  "types": "./env.d.ts",
  "exports": {
    ".": {
      "types": "./env.d.ts",
      "import": "./dist/env.js",
      "default": "./dist/env.js"
    },
    "./env": {
      "types": "./env.d.ts",
      "import": "./dist/env.js",
      "default": "./dist/env.js"
    }
  },
  "files": [
    "dist",
    "env.d.ts"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist tsconfig.tsbuildinfo || rm -rf dist tsconfig.tsbuildinfo",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "rimraf": "^6.1.0",
    "typescript": "^5.9.3"
  }
}
